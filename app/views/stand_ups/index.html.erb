<h1 class="text-center">StandUps</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="panel">
      <table class="table table-hover text-center">
        <tr>
          <th class="text-center">User</th>
          <th class="text-center">StandUp</th>
        </tr>

        <% @standUps.each do |standUp| %>
          <tr>
            <td><%= link_to current_user.email %></td>
            <td><%= link_to standUp.created_at.to_formatted_s(:long), stand_up_path(standUp) %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
